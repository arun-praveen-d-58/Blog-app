

<div class="card  align-items-center" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title m-3">#<%=tag.name%></h5>
    <%= link_to 'Edit',  edit_tag_path(tag.id.to_i), class: "btn btn-primary" %>
    <%= link_to 'Delete', tag_path(tag.id.to_i),data:{turbo_method: :delete,confirm:"Are you sure to delete?"}, class: "btn btn-danger"  %>
  </div>
</div>